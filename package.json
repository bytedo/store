  "description": "indexedDB 操作库",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build:next": "esbuild src/index.js --minify --bundle --format=esm --target=esnext --outfile=dist/index.js",
